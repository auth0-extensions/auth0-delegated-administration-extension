{
  "name": "auth0-delegated-admin",
  "version": "2.6.2",
  "description": "Delegated Administration Dashboard",
  "engines": {
    "node": "6.x"
  },
  "main": "server.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run client:build && npm run extension:build",
    "client:build": "cross-env NODE_ENV=production webpack --config ./build/webpack/config.prod.js --colors -p",
    "extension:build": "a0-ext build:server ./webtask.js ./dist && cp ./dist/auth0-delegated-admin.extension.$npm_package_version.js ./build/bundle.js",
    "serve:dev": "cross-env NODE_ENV=development nodemon -e js --ignore assets/app/ --ignore build/webpack/ --ignore client/ --ignore server/data.json --ignore node_modules/ ./build/webpack/server.js",
    "serve:prod": "cross-env NODE_ENV=production node index.js",
    "test": "cross-env NODE_ENV=test nyc mocha tests/mocha.js ./tests/**/*.tests.js",
    "test:watch": "cross-env NODE_ENV=test mocha tests/mocha.js ./tests/**/*.tests.js --watch",
    "test:pre": "npm run test:clean && npm run lint:js",
    "test:clean": "rimraf ./coverage && rimraf ./.nyc_output",
    "extension:size": "cross-env NODE_ENV=production webpack -p --config ./build/extension/webpack.config.js --json > ./build/extension/bundle-size.json && node ./build/extension/bundle-size.js"
  },
  "keywords": [
    "auth0"
  ],
  "auth0-extension": {
    "bundleModules": false,
    "settings": {
      "WARN_DB_SIZE": 409600,
      "MAX_MULTISELECT_USERS": 5,
      "MULTISELECT_DEBOUNCE_MS": 250,
      "PER_PAGE": 10
    }
  },
  "author": "Sandrino Di Mattia",
  "license": "MIT",
  "dependencies": {
    "auth0": "^2.4.0",
    "auth0-extension-express-tools": "^1.1.5",
    "auth0-extension-tools": "^1.3.1",
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.1",
    "ejs": "^2.4.2",
    "express": "4.14.0",
    "lodash": "^3.10.1",
    "lru-memoizer": "1.10.0",
    "moment": "^2.13.0",
    "morgan": "^1.7.0",
    "request": "^2.34.0",
    "require-like": "^0.1.2",
    "safe-eval": "^0.3.0",
    "superagent": "1.2.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "auth0-extensions-cli": "git+https://github.com/auth0-extensions/auth0-extensions-cli.git",
    "autoprefixer": "^6.5.1",
    "axios": "^0.14.0",
    "babel": "6.23.0",
    "babel-core": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "babel-runtime": "6.23.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "codemirror": "^5.19.0",
    "classnames": "^2.2.5",
    "cross-env": "5.0.0",
    "css-loader": "0.28.2",
    "eslint": "^3.19.0",
    "eslint-config-auth0": "10.2.0",
    "eslint-config-auth0-base": "^12.1.0",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "7.0.1",
    "expect": "^1.20.2",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.11.1",
    "history": "^2.1.2",
    "imports-loader": "0.7.1",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.4",
    "jwt-decode": "^2.0.1",
    "mocha": "3.4.2",
    "ms": "^0.7.1",
    "nconf": "^0.8.4",
    "nock": "9.0.13",
    "nodemon": "^1.11.0",
    "nyc": "10.3.2",
