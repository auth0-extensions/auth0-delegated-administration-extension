    "redux-devtools-log-monitor": "^1.1.1",
    "redux-simple-router": "^2.0.4",
    "redux-static": "^1.0.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.23.1",
    "supertest": "^2.0.1",
    "webpack": "^5.90.1"
  },
  "overrides": {
    "react": "15.5.0",
    "cookie-parser": "1.4.6",
    "cheerio": "1.0.0-rc.12"
  }
}
